keycode  32 = o O U03BF U039F
keycode  33 = p P U03C0 U03A0
keycode  38 = a A U03B1 U0391
keycode  39 = s S U03C3 U03A3
keycode  40 = d D U03B4 U0394
keycode  41 = f F U03C6 U03A6
