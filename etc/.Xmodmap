keycode  17 = 8 asterisk asterisk U1370
keycode  18 = 9 parenleft parenleft U1371
keycode  19 = 0 parenright parenright U1372
keycode  23 = Tab ISO_Left_Tab
keycode  24 = q Q U03B8 U0398
keycode  25 = w W U03C9 U03A9
keycode  26 = e E U03B5 U0395
